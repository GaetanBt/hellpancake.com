{%- import "components/ui.njk" as ui with context -%}

<nav  role="navigation">
  <ul class="Nav flex flex-wrap justify-content-center list-reset">
    {%- for entry in collections.all | eleventyNavigation -%}
      <li class="Nav-item{{ " Nav-item--home" if entry.key == "Home" }}">
        {{ ui.navLink(entry) }}
      </li>
    {%- endfor -%}
  </ul>
</nav>
